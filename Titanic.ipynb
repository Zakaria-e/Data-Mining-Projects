    "\n",
    "import pandas as pd\n",
    "import math \n",
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "import sklearn\n",
    "import numpy as np\n",
    "from sklearn.model_selection import train_test_split\n",
    "import statsmodels.api as sm\n",
    "from sklearn.linear_model import LogisticRegression\n",
    "from sklearn.model_selection import cross_val_score\n",
    "import warnings\n",
    "warnings.filterwarnings(\"ignore\", category=FutureWarning)\n",
    "from pandas.plotting import scatter_matrix\n",
    "\n",
    "\n",
    "#set up data\n",
    "pd.set_option('display.max_columns', 500)\n",
    "tit=pd.read_csv(\'https://raw.githubusercontent.com/chonorat/Titanic/master/train.csv')\n",
    "test=pd.read_csv(\"https://raw.githubusercontent.com/chonorat/Titanic/master/test.csv")\n",
    "tit.hist(bins=50,figsize=(20,15))\n",
    "scatter_matrix(tit[plotted_var], figsize=(17, 11))\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "#FeatureEngineering\n",
    "#Ticket_Sum\n",
    "test[\"data\"]=\"test\"\n",
    "tit[\"data\"]=\"train\"\n",
    "tit2=tit\n",
    "tit2=tit2.append(test, ignore_index=True)\n",
    "tit2[\"PlaceHold\"]=1\n",
    "tit2['TicketSum'] = tit2['PlaceHold'].groupby(tit2['Ticket']).transform('sum')\n",
    "test = tit2[tit2['data'] ==\"test\"]\n",
    "tit = tit2[tit2['data'] ==\"train\"]\n",
    "\n",
    "#Class\n",
    "test[\"Pclass\"]=test.Pclass.fillna(test.Pclass.median())\n",
    "tit[\"Pclass\"]=tit.Pclass.fillna(tit.Pclass.median())\n",
    "\n",
    "\n",
    "#sex\n",
    "sex_dummies=pd.get_dummies(tit[\"Sex\"],prefix=\"Sex\")\n",
    "tit=tit.join(sex_dummies)\n",
    "tit=tit.drop([\"Sex_male\"],axis=1)\n",
    "sex_dummies1=pd.get_dummies(test[\"Sex\"],prefix=\"Sex\")\n",
    "test=test.join(sex_dummies1)\n",
    "test=test.drop([\"Sex_male\"],axis=1)\n",
    "\n",
    "\n",
    "#Child\n",
    "tit[\"Child\"]=1\n",
    "tit.loc[tit.Age>7,\"Child\"]=0\n",
    "test[\"Child\"]=1\n",
    "test.loc[test.Age>7,\"Child\"]=0\n",
    "\n",
    "\n",
    "\n",
    "#Embarked\n",
    "tit.Embarked=tit.Embarked.fillna(0)\n",
    "tit.loc[tit.Embarked==\"S\",\"Embarked\"]=0\n",
    "tit.loc[tit.Embarked==\"C\",\"Embarked\"]=1\n",
    "tit.loc[tit.Embarked==\"Q\",\"Embarked\"]=2\n",
    "test.Embarked=test.Embarked.fillna(0)\n",
    "test.loc[test.Embarked==\"S\",\"Embarked\"]=0\n",
    "test.loc[test.Embarked==\"C\",\"Embarked\"]=1\n",
    "test.loc[test.Embarked==\"Q\",\"Embarked\"]=2\n",
    "tit[\"EmbarkScale\"]=tit.Embarked.replace({0:0,2:1,1:0})\n",
    "test[\"EmbarkScale\"]=test.Embarked.replace({0:0,2:1,1:0})\n",
    "\n",
    "#famliy\n",
    "tit[\"SibSp\"]=tit.SibSp.fillna(tit.SibSp.mean())\n",
    "test[\"SibSp\"]=test.SibSp.fillna(test.SibSp.median())\n",
    "tit[\"j\"]=tit.SibSp+tit.Parch\n",
    "test[\"j\"]=test.SibSp+test.Parch\n",
    "tit[\"FamilyScale\"]=tit.j.replace({3:1,2:1,1:1,0:2,4:2,5:2,6:2,7:3,10:3})\n",
    "test[\"FamilyScale\"]=test.j.replace({3:1,2:1,1:1,0:2,4:2,5:2,6:2,7:3,10:3})\n",
    "\n",
    "\n",
    "#Age\n",
    "tit[\"Age\"]=tit.Age.fillna(tit.Age.mean())\n",
    "test[\"Age\"]=test.Age.fillna(test.Age.mean())\n",
    "bins = [0, 3, 10,16,22, 32,42,52, 62,70, np.inf]\n",
    "names = ['<3', '3-10','10-16','16-22', '22-32', '32-42','42-52','52-62', '62-70','70+']\n",
    "tit['AgeRange'] = pd.cut(tit['Age'], bins, labels=names)\n",
    "ax = sns.countplot(x=\"AgeRange\",hue=\"Survived\", data=tit)\n",
    "tit[\"AgeScale\"]=tit.AgeRange.replace({'<3':1, '3-10':2,'10-16':3,'16-22':6, '22-32':6, '32-42':4,'42-52':6,'52-62':4, '62-70':7,'70+':5})\n",
    "\n",
    "#Cabin\n",
    "test[\"Cabin\"] = test.Cabin.fillna('Z')\n",
    "test['CA'] = test['Cabin'].astype(str).str[0]\n",
    "tit.Cabin = tit.Cabin.fillna('Z')\n",
    "tit['CA'] = tit['Cabin'].astype(str).str[0]\n",
    "test[\"CabinScale\"]=test.CA.replace({\"Z\": 1,\"C\": 2,\"E\": 2,\"D\": 2,\"B\": 2,\"G\": 2,\"A\": 1,\"F\": 2,\"T\": 1})\n",
    "tit[\"CabinScale\"]=tit.CA.replace({\"Z\": 1,\"C\": 2,\"E\": 2,\"D\": 2,\"B\": 2,\"G\": 2,\"A\": 1,\"F\": 2,\"T\": 1})\n",
    "\n",
    "#ticketsum\n",
    "test[\"TicketScale\"]=test.TicketSum.replace({11:1,7:1,6:1,5:1,8:1,1:1,2:2,3:2,4:2})\n",
    "tit[\"TicketScale\"]=tit.TicketSum.replace({11:1,7:1,6:1,5:1,8:1,1:1,2:2,3:2,4:2})\n",
    "\n",
    "#ticketlen\n",
    "tit['TickLen']=tit.Ticket.apply(len)\n",
    "test['TickLen']=test.Ticket.apply(len)\n",
    "test[\"TicLenScale\"]=test.TickLen.replace({5:3,8:2,3:1,4:1,6:1,7:1,9:1,10:1,11:1,12:1,13:1,15:1,16:1,17:1,18:0})\n",
    "tit[\"TicLenScale\"]=tit.TickLen.replace({5:3,8:2,3:1,4:1,6:1,7:1,9:1,10:1,11:1,12:1,13:1,15:1,16:1,17:1,18:0})\n",
    "\n",
    "\n",
    "#fare\n",
    "tit[\"Fare\"]=tit.Fare.fillna(tit.Fare.median())\n",
    "test[\"Fare\"]=test.Fare.fillna(test.Fare.mean())\n",
    "bins = [0, 50, 100,200,300, 500, np.inf]\n",
    "names = ['<50', '50-100','100-200','200-300', '300-500', '500+']\n",
    "tit['FareRange'] = pd.cut(tit['Fare'], bins, labels=names)\n",
    "tit[\"FareScale\"]=tit.FareRange.replace({'<50':1, '50-100':2,'100-200':2,'200-300':2, '300-500':2, '500+':2})\n",
    "ax = sns.countplot(x=\"FareScale\",hue=\"Survived\", data=tit)\n",
    "test['FareRange'] = pd.cut(test['Fare'], bins, labels=names)\n",
    "test[\"FareScale\"]=test.FareRange.replace({'<50':1, '50-100':2,'100-200':2,'200-300':2, '300-500':2, '500+':2})\n",
    "\n",
    "\n",
    "#Title\n",
    "test['Title'] = test.Name.apply(lambda name: name.split(',')[1].split('.')[0].strip())\n",
    "test[\"titleform\"]=test.Title.replace({\n",
    "    \"Capt\":       \"Officer\",\n",
    "    \"Col\":        \"Officer\",\n",
    "    \"Major\":      \"Officer\",\n",
    "    \"Jonkheer\":   \"Royalty\",\n",
    "    \"Don\":        \"Royalty\",\n",
    "    \"Sir\" :       \"Royalty\",\n",
    "    \"Dr\":         \"Officer\",\n",
    "    \"Rev\":        \"Officer\",\n",
    "    \"the Countess\":\"Royalty\",\n",
    "    \"Dona\":       \"Royalty\",\n",
    "    \"Mme\":        \"Mrs\",\n",
    "    \"Mlle\":       \"Miss\",\n",
    "    \"Ms\":         \"Mrs\",\n",
    "    \"Mr\" :        \"Mr\",\n",
    "    \"Mrs\" :       \"Mrs\",\n",
    "    \"Miss\" :      \"Miss\",\n",
    "    \"Master\" :    \"Master\",\n",
    "    \"Lady\" :      \"Royalty\"\n",
    "})\n",
    "test[\"TitScale\"]=test.titleform.replace({\"Mr\": 1,\"Officer\": 2,\"Master\": 3,\"Royalty\": 3,\"Mrs\": 4,\"Miss\": 4})\n",
    "tit['Title'] = tit.Name.apply(lambda name: name.split(',')[1].split('.')[0].strip())\n",
    "tit[\"titleform\"]=tit.Title.replace({\n",
    "    \"Capt\":       \"Officer\",\n",
    "    \"Col\":        \"Officer\",\n",
    "    \"Major\":      \"Officer\",\n",
    "    \"Jonkheer\":   \"Royalty\",\n",
    "    \"Don\":        \"Royalty\",\n",
    "    \"Sir\" :       \"Royalty\",\n",
    "    \"Dr\":         \"Officer\",\n",
    "    \"Rev\":        \"Officer\",\n",
    "    \"the Countess\":\"Royalty\",\n",
    "    \"Dona\":       \"Royalty\",\n",
    "    \"Mme\":        \"Mrs\",\n",
    "    \"Mlle\":       \"Miss\",\n",
    "    \"Ms\":         \"Mrs\",\n",
    "    \"Mr\" :        \"Mr\",\n",
    "    \"Mrs\" :       \"Mrs\",\n",
    "    \"Miss\" :      \"Miss\",\n",
    "    \"Master\" :    \"Master\",\n",
    "    \"Lady\" :      \"Royalty\"\n",
    "})\n",
    "tit[\"TitScale\"]=tit.titleform.replace({\"Mr\": 1,\"Officer\": 2,\"Master\": 3,\"Royalty\": 3,\"Mrs\": 4,\"Miss\": 4})\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "#Data_Exploration\n",
    "#plots\n",
    "j=tit[[\"Survived\",\"Sex_female\",\"CabinScale\",\"Pclass\",\"Child\",\"TitScale\",\"EmbarkScale\",\"FamilyScale\",\"TicLenScale\",\"TicketSum\"]]\n",
    "for i, col in enumerate(j.columns):\n",
    "    plt.figure(i)\n",
    "    sns.countplot(x=col,hue=\"Survived\", data=j)\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "#Define_Variables\n",
    "j=tit[[\"Sex_female\",\"CabinScale\",\"Pclass\",\"Child\",\"TitScale\",\"EmbarkScale\",\"FamilyScale\",\"TicLenScale\",\"TicketSum\"]]\n",
    "\n",
    "jtest=test[[\"Sex_female\",\"CabinScale\",\"Pclass\",\"Child\",\"TitScale\",\"EmbarkScale\",\"FamilyScale\",\"TicLenScale\",\"TicketSum\"]]\n",
    "\n",
    "\n",
    "\n",
    "#Model_Evaluation\n",
    "def model():\n",
    "    global j\n",
    "    y=tit[\"Survived\"]\n",
    "    logisticRegr = LogisticRegression()\n",
    "\n",
    "   \n",
    "    #Evaluate\n",
    "    CVscores=sklearn.model_selection.cross_val_score(logisticRegr,j,y,cv=10)\n",
    "    CVavg=np.mean(CVscores)\n",
    "    CVstd=np.std(CVscores)\n",
    "    ScorePrint=('Scores-',np.round(CVscores,4),'Average-',CVavg,'Std-',CVstd)\n",
    "    print(\"SCORES-\",np.round(CVscores,4),'\\n')\n",
    "    print(\"AVERAGE-\",np.round(CVavg,4),'\\n')\n",
    "    print(\"STD.DEV.-\",np.round(CVstd,4),'\\n')\n",
    "    import statsmodels.api as sm\n",
    "    logit_model=sm.Logit(y,j)\n",
    "    result=logit_model.fit()\n",
    "    print(result.summary2())\n",
    "\n",
    "\n",
    "#Save_Function\n",
    "def Save():\n",
    "    logisticRegr = LogisticRegression()\n",
    "    logisticRegr.fit(j, y)\n",
    "    Y_pred=logisticRegr.predict(jtest)\n",
    "    submission = pd.DataFrame({'PassengerId':test['PassengerId'],'Survived':Y_pred})\n",
    "    submission.to_csv(\"/Users/chono/OneDrive/Desktop/Spring 19/Data Mining/sub76.csv\",index=False)\n",
    "\n",
    "\n",
    "\n",
    "\n"
   ]
  }
